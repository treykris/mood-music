{
    "extends": [
        "eslint:recommended",
        "plugin:import/errors",
        "prettier"
    ],
    "parserOptions": {
        "parser": "babel-eslint",
        "sourceType": "module",
        "ecmaVersion": 8
    },
    "plugins": ["import"],
    "rules": {
        "no-console": 1
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    }
}